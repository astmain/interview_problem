{
  "name": "【V3】君臣论-电商插件",
  "description": "",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "tabs", "contextMenus", "scripting"],
  "action": {
    "default_title": "popup.html",
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "icons": { "16": "logo.png", "48": "logo.png", "128": "logo.png" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  // "web_accessible_resources": [
  //   {
  //     "resources": ["background.js"],
  //     "matches": ["<all_urls>"]
  //   }
  // ]
}
